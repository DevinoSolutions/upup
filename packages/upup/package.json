{
    "name": "upup-react-file-uploader",
    "author": "Devino Solutions",
    "license": "MIT",
    "version": "1.5.2",
    "publishConfig": {
        "access": "public"
    },
    "readme": "README.md",
    "main": "dist/index.js",
    "module": "dist/upup.esm.js",
    "types": "./dist/index.browser.d.ts",
    "typings": "./dist/index.browser.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.browser.d.ts",
            "import": "./dist/index.browser.mjs",
            "require": "./dist/index.browser.js"
        },
        "./server": {
            "types": "./dist-node/index.node.d.ts",
            "default": "./dist-node/index.node.js"
        },
        "./shared": {
            "types": "./dist/shared/types/StorageSDK.d.ts"
        },
        "./styles": "./dist/tailwind-prefixed.css"
    },
    "typesVersions": {
        "*": {
            "*": [
                "./dist/index.d.ts"
            ]
        }
    },
    "browser": "./dist/upup.esm.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/DevinoSolutions/upup.git"
    },
    "packageManager": "pnpm@10.11.0",
    "keywords": [
        "react",
        "typescript",
        "aws/sdk",
        "s3",
        "file",
        "uploader",
        "react-uploader",
        "one-drive",
        "google-drive",
        "pako",
        "gzip",
        "compress",
        "decompress"
    ],
    "scripts": {
        "dev": "concurrently \"pnpm run storybook\" \"pnpm run dev:server\" --names \"storybook,server\" --prefix-colors \"blue,green\"",
        "dev:build": "pnpm run build:css && pnpm exec tsup --config tsup.config.ts --watch",
        "dev:server": "dotenv -e ../../.env -e ../../local-dev/.env.ports -- node ./server/index.js",
        "build:css": "pnpm exec postcss src/frontend/tailwind.css -o ./dist/tailwind-prefixed.css",
        "build": "pnpm run prebuild && pnpm exec tsup --config tsup.config.ts && pnpm run build:css",
        "prebuild": "node -e \"const fs=require('fs');['dist','dist-node'].forEach(d=>fs.rmSync(d,{recursive:true,force:true}));\" && pnpm run lint",
        "test": "pnpm exec jest",
        "test:watch": "pnpm exec jest --watch",
        "test:coverage": "pnpm exec jest --coverage",
        "test:ci": "pnpm exec jest --coverage --ci --maxWorkers=2",
        "test:e2e": "pnpm exec playwright test",
        "test:e2e:ui": "pnpm exec playwright test --ui",
        "playwright:install": "pnpm exec playwright install --with-deps",
        "lint": "pnpm exec eslint src/",
        "lint:fix": "pnpm exec eslint src/ --fix",
        "size": "pnpm exec size-limit",
        "prettier-check": "pnpm exec prettier --check .",
        "prettier-write": "pnpm exec prettier --write .",
        "analyze": "pnpm exec size-limit --why",
        "storybook": "dotenv -e ../../.env -e ../../local-dev/.env.ports -- pnpm exec storybook dev -p 53050",
        "build-storybook": "pnpm exec storybook build",
        "ci": "pnpm run prettier-check && pnpm run build",
        "release": "pnpm run prettier-check && pnpm run build && pnpm exec changeset publish",
        "prepare": "pnpm run build"
    },
    "size-limit": [
        {
            "path": "dist/upup.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/upup.esm.js",
            "limit": "10 KB"
        }
    ],
    "peerDependencies": {
        "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
        "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.23.3",
        "@babel/plugin-proposal-optional-chaining": "^7.21.0",
        "@babel/preset-env": "^7.23.3",
        "@babel/preset-react": "^7.23.3",
        "@babel/preset-typescript": "^7.23.3",
        "@changesets/cli": "^2.26.2",
        "@chromatic-com/storybook": "^3",
        "@emotion/is-prop-valid": "^1.3.1",
        "@eslint/compat": "^1.2.5",
        "@eslint/js": "^9.19.0",
        "@playwright/test": "^1.47.2",
        "@rollup/plugin-alias": "^5.1.1",
        "@rollup/plugin-node-resolve": "^16.0.0",
        "@rollup/plugin-replace": "^6.0.2",
        "@size-limit/preset-small-lib": "^10.0.3",
        "@storybook/addon-docs": "^8.5.2",
        "@storybook/addon-essentials": "^8.5.2",
        "@storybook/addon-interactions": "^8.5.2",
        "@storybook/addon-links": "^8.5.2",
        "@storybook/addon-styling-webpack": "^1.0.1",
        "@storybook/addon-themes": "^8.5.2",
        "@storybook/addon-webpack5-compiler-babel": "^3.0.5",
        "@storybook/blocks": "^8.5.2",
        "@storybook/react": "^8.5.2",
        "@storybook/react-webpack5": "^8.5.2",
        "@storybook/test": "^8.5.2",
        "@tailwindcss/container-queries": "^0.1.1",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.0.1",
        "@testing-library/user-event": "^14.5.2",
        "@types/jest": "29.5.11",
        "@types/node": "^20.10.0",
        "@types/pako": "^2.0.3",
        "@types/react": ">=16.8.0",
        "@types/react-dom": ">=16.8.0",
        "@types/uuid": "^9.0.7",
        "@typescript-eslint/eslint-plugin": "^8.22.0",
        "@typescript-eslint/parser": "^8.22.0",
        "autoprefixer": "^10.4.20",
        "babel-loader": "^9.1.3",
        "concurrently": "^9.2.1",
        "css-loader": "^7.1.2",
        "dotenv": "^17.2.1",
        "eslint": "^9.19.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.3",
        "eslint-plugin-react": "^7.37.4",
        "eslint-plugin-react-hooks": "^5.1.0",
        "globals": "^15.14.0",
        "highlight.js": "10.7.3",
        "husky": "^8.0.3",
        "identity-obj-proxy": "^3.0.0",
        "jest": "29.7.0",
        "jest-canvas-mock": "^2.5.2",
        "jest-environment-jsdom": "29.7.0",
        "lowlight": "1.20.0",
        "postcss": "^8.5.2",
        "postcss-cli": "^11.0.1",
        "postcss-loader": "^8.1.1",
        "postcss-prefix-selector": "^2.1.1",
        "prettier": "3.0.3",
        "prettier-plugin-organize-imports": "^3.2.4",
        "prettier-plugin-tailwindcss": "^0.5.7",
        "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
        "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
        "react-syntax-highlighter": "15.4.5",
        "remark-gfm": "^4.0.0",
        "rollup-plugin-analyzer": "^4.0.0",
        "rollup-plugin-postcss": "^4.0.2",
        "size-limit": "^10.0.3",
        "storybook": "^8.5.2",
        "style-loader": "^4.0.0",
        "tailwindcss": "^3.4.17",
        "ts-jest": "29.1.1",
        "tslib": "^2.6.2",
        "tsup": "^8.4.0",
        "typescript": "^5.3.2",
        "typescript-eslint": "^8.21.0",
        "webpack": "5.89.0",
        "zod": "^3.22.4"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.689.0",
        "@aws-sdk/s3-request-presigner": "^3.689.0",
        "@aws-sdk/xhr-http-handler": "^3.451.0",
        "@azure/identity": "^4.5.0",
        "@azure/msal-browser": "^3.5.0",
        "@azure/storage-blob": "^12.25.0",
        "@google-cloud/storage": "^7.14.0",
        "@microsoft/microsoft-graph-client": "^3.0.7",
        "@types/gapi": "^0.0.47",
        "babel-plugin-module-resolver": "^5.0.0",
        "clsx": "^2.1.1",
        "cors": "^2.8.5",
        "dropbox": "^10.34.0",
        "express": "^5.1.0",
        "framer-motion": "^12.0.6",
        "load-script": "^2.0.0",
        "pako": "^2.1.0",
        "react-icons": "^4.12.0",
        "react-webcam": "^7.2.0",
        "tailwind-merge": "^2.6.0",
        "truncate": "^3.0.0",
        "use-debounce": "^10.0.4",
        "uuid": "^9.0.1"
    }
}
