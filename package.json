{
    "name": "@bassem97/upup",
    "author": "Bassem Jadoui",
    "license": "MIT",
    "version": "2.4.0",
    "readme": "README.md",
    "main": "dist/index.js",
    "module": "dist/upup.esm.js",
    "types": "dist/index.d.ts",
    "typings": "dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/uNotesOfficial/upup.git"
    },
    "packageManager": "pnpm@8.10.5",
    "keywords": [
        "react",
        "typescript",
        "aws/sdk",
        "s3",
        "file",
        "uploader",
        "react-uploader",
        "one-drive",
        "google-drive",
        "pako",
        "gzip",
        "compress",
        "decompress"
    ],
    "scripts": {
        "start": "pnpm run prebuild && tsdx watch",
        "prebuild": "node -p \"'export const LIB_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts",
        "build": "pnpm run prebuild && tsdx build",
        "test": "tsdx test --passWithNoTests",
        "lint": "tsdx lint",
        "prepare": "tsdx build",
        "size": "size-limit",
        "prettier-check": "prettier --check .",
        "prettier-write": "prettier --write .",
        "analyze": "size-limit --why",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "ci": "pnpm run prettier-check && pnpm run build",
        "release": "pnpm run prettier-check && pnpm run build && changeset publish"
    },
    "size-limit": [
        {
            "path": "dist/upup.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/upup.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@babel/core": "^7.23.3",
        "@babel/plugin-proposal-optional-chaining": "^7.21.0",
        "@babel/preset-env": "^7.23.3",
        "@babel/preset-react": "^7.23.3",
        "@babel/preset-typescript": "^7.23.3",
        "@changesets/cli": "^2.26.2",
        "@rollup/plugin-replace": "^5.0.5",
        "@size-limit/preset-small-lib": "^10.0.3",
        "@storybook/addon-essentials": "^7.5.3",
        "@storybook/addon-info": "^5.3.21",
        "@storybook/addon-links": "^7.5.3",
        "@storybook/addon-mdx-gfm": "^7.5.3",
        "@storybook/addon-styling": "^1.3.7",
        "@storybook/addons": "^7.5.3",
        "@storybook/react": "^7.5.3",
        "@storybook/react-webpack5": "^7.5.3",
        "@types/node": "^20.10.0",
        "@types/pako": "^2.0.3",
        "@types/react": "^18.2.39",
        "@types/react-dom": "^18.2.17",
        "@types/uuid": "^9.0.7",
        "@typescript-eslint/eslint-plugin": "^6.13.0",
        "@typescript-eslint/parser": "^6.13.0",
        "autoprefixer": "^10.4.16",
        "babel-loader": "^9.1.3",
        "husky": "^8.0.3",
        "postcss": "^8.4.31",
        "prettier": "3.0.3",
        "prettier-plugin-organize-imports": "^3.2.4",
        "prettier-plugin-tailwindcss": "^0.5.7",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "remark-gfm": "^4.0.0",
        "rollup-plugin-analyzer": "^4.0.0",
        "rollup-plugin-postcss": "^4.0.2",
        "size-limit": "^10.0.3",
        "storybook": "^7.5.3",
        "tailwindcss": "^3.3.5",
        "tsdx": "^0.14.1",
        "tslib": "^2.6.2",
        "typescript": "^5.3.2",
        "webpack": "5.89.0",
        "zod": "^3.22.4"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.458.0",
        "@aws-sdk/xhr-http-handler": "^3.451.0",
        "@azure/msal-browser": "^3.5.0",
        "@emotion/styled": "^11.11.0",
        "@microsoft/microsoft-graph-client": "^3.0.7",
        "@mui/material": "^5.14.18",
        "@types/gapi": "^0.0.47",
        "authProviders": "link:@microsoft/microsoft-graph-client/authProviders",
        "babel-plugin-module-resolver": "^5.0.0",
        "framer-motion": "^10.16.5",
        "load-script": "^2.0.0",
        "pako": "^2.1.0",
        "react-file-viewer": "^1.2.1",
        "react-icons": "^4.12.0",
        "react-webcam": "^7.2.0",
        "use-debounce": "^10.0.4",
        "uuid": "^9.0.1"
    },
    "peerDependencies": {
        "@emotion/react": "^11.11.1",
        "react": "18.2.0",
        "react-dom": "18.2.0"
    },
    "pnpm": {
        "overrides": {
            "fast-xml-parser": "^4.3.2",
            "prismjs": "^1.29.0",
            "tough-cookie": "^4.1.3",
            "jsdom": "22.1.0",
            "node-notifier": "10.0.1",
            "highlight.js": "11.9.0",
            "node-fetch": "2.6.7"
        }
    }
}
