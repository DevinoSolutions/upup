{
    "name": "@bassem97/upup",
    "author": "Bassem Jadoui",
    "license": "MIT",
    "version": "2.2.29",
    "readme": "README.md",
    "main": "dist/index.js",
    "module": "dist/upup.esm.js",
    "types": "dist/index.d.ts",
    "typings": "dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/uNotesOfficial/upup.git"
    },
    "keywords": [
        "react",
        "typescript",
        "aws/sdk",
        "s3",
        "file",
        "uploader",
        "react-uploader",
        "one-drive",
        "google-drive",
        "pako",
        "gzip",
        "compress",
        "decompress"
    ],
    "scripts": {
        "start": "pnpm run prebuild && tsdx watch",
        "prebuild": "node -p \"'export const LIB_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts",
        "build": "pnpm run prebuild && tsdx build",
        "test": "tsdx test --passWithNoTests",
        "lint": "tsdx lint",
        "prepare": "tsdx build",
        "size": "size-limit",
        "prettier-check": "prettier --check .",
        "prettier-write": "prettier --write .",
        "analyze": "size-limit --why",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "ci": "pnpm run prettier-check && pnpm run build",
        "release": "pnpm run prettier-check && pnpm run build && changeset publish"
    },
    "size-limit": [
        {
            "path": "dist/upup.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/upup.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@aws-sdk/client-s3": "^3.271.0",
        "@babel/core": "^7.22.10",
        "@babel/preset-env": "^7.23.2",
        "@babel/preset-react": "^7.22.5",
        "@babel/preset-typescript": "^7.22.5",
        "@changesets/cli": "^2.26.2",
        "@rollup/plugin-replace": "^5.0.2",
        "@size-limit/preset-small-lib": "^8.2.4",
        "@storybook/addon-essentials": "^7.3.0",
        "@storybook/addon-info": "^5.3.21",
        "@storybook/addon-links": "^7.3.0",
        "@storybook/addon-mdx-gfm": "^7.3.0",
        "@storybook/addon-styling": "^1.3.6",
        "@storybook/addons": "^7.3.0",
        "@storybook/react": "^7.3.0",
        "@storybook/react-webpack5": "^7.3.0",
        "@types/node": "^20.8.7",
        "@types/pako": "^2.0.0",
        "@types/react": "^18.2.20",
        "@types/react-dom": "^18.2.7",
        "@types/uuid": "^9.0.3",
        "autoprefixer": "^10.4.15",
        "babel-loader": "^9.1.3",
        "husky": "^8.0.3",
        "pako": "^2.1.0",
        "postcss": "^8.4.31",
        "prettier": "3.0.0",
        "remark-gfm": "^3.0.1",
        "rollup-plugin-analyzer": "^4.0.0",
        "rollup-plugin-postcss": "^4.0.2",
        "size-limit": "^8.2.6",
        "storybook": "^7.3.0",
        "styled-components": "^6.0.7",
        "tailwindcss": "^3.3.3",
        "tsdx": "^0.14.1",
        "tslib": "^2.5.0",
        "typescript": "^5.0.4",
        "webpack": "5.88.2",
        "zod": "^3.21.4"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.271.0",
        "@aws-sdk/xhr-http-handler": "^3.271.0",
        "@emotion/styled": "^11.11.0",
        "@mui/material": "^5.14.13",
        "@types/gapi": "^0.0.44",
        "babel-plugin-module-resolver": "^5.0.0",
        "framer-motion": "^10.15.2",
        "load-script": "^2.0.0",
        "react-icons": "^4.10.1",
        "react-webcam": "^7.1.1",
        "uuid": "^9.0.0"
    },
    "peerDependencies": {
        "@emotion/react": "^11.11.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    }
}
