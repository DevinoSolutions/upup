{
    "name": "upup-react-file-uploader",
    "author": "Devino Solutions",
    "license": "MIT",
    "version": "1.1.1",
    "publishConfig": {
        "access": "public"
    },
    "readme": "README.md",
    "main": "dist/index.js",
    "module": "dist/upup.esm.js",
    "types": "./dist/index.browser.d.ts",
    "typings": "./dist/index.browser.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.browser.d.ts",
            "browser": "./dist/browser.esm.js",
            "import": "./dist/browser.esm.js",
            "require": "./dist/browser.cjs.development.js"
        },
        "./server": {
            "types": "./dist/index.node.d.ts",
            "default": "./dist/node.node.js"
        },
        "./shared": {
            "types": "./dist/shared/types/StorageSDK.d.ts"
        },
        "./styles": "./dist/upup.css"
    },
    "typesVersions": {
        "*": {
            "*": [
                "./dist/index.d.ts"
            ]
        }
    },
    "browser": "./dist/upup.esm.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/DevinoSolutions/upup.git"
    },
    "packageManager": "pnpm@8.10.5",
    "keywords": [
        "react",
        "typescript",
        "aws/sdk",
        "s3",
        "file",
        "uploader",
        "react-uploader",
        "one-drive",
        "google-drive",
        "pako",
        "gzip",
        "compress",
        "decompress"
    ],
    "scripts": {
        "start": "pnpm run prebuild && tsdx watch",
        "build:browser": "tsdx build --entry ./src/index.browser.ts --tsconfig ./tsconfig.browser.json --format esm,cjs --name browser",
        "build:node": "tsdx build --entry ./src/index.node.ts --tsconfig ./tsconfig.node.json --format cjs --name node",
        "build": "node scripts/build.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:ci": "jest --coverage --ci --maxWorkers=2",
        "lint": "eslint src/",
        "lint:fix": "eslint src/ --fix",
        "size": "size-limit",
        "prettier-check": "prettier --check .",
        "prettier-write": "prettier --write .",
        "analyze": "size-limit --why",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "ci": "pnpm run prettier-check && pnpm run build",
        "release": "pnpm run prettier-check && pnpm run build && changeset publish"
    },
    "size-limit": [
        {
            "path": "dist/upup.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/upup.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@babel/core": "^7.23.3",
        "@babel/plugin-proposal-optional-chaining": "^7.21.0",
        "@babel/preset-env": "^7.23.3",
        "@babel/preset-react": "^7.23.3",
        "@babel/preset-typescript": "^7.23.3",
        "@changesets/cli": "^2.26.2",
        "@chromatic-com/storybook": "^3",
        "@emotion/is-prop-valid": "^1.3.1",
        "@eslint/compat": "^1.2.5",
        "@eslint/js": "^9.19.0",
        "@rollup/plugin-alias": "^5.1.1",
        "@rollup/plugin-node-resolve": "^16.0.0",
        "@rollup/plugin-replace": "^6.0.2",
        "@size-limit/preset-small-lib": "^10.0.3",
        "@storybook/addon-docs": "^8.5.2",
        "@storybook/addon-essentials": "^8.5.2",
        "@storybook/addon-interactions": "^8.5.2",
        "@storybook/addon-links": "^8.5.2",
        "@storybook/addon-styling-webpack": "^1.0.1",
        "@storybook/addon-themes": "^8.5.2",
        "@storybook/addon-webpack5-compiler-babel": "^3.0.5",
        "@storybook/blocks": "^8.5.2",
        "@storybook/react": "^8.5.2",
        "@storybook/react-webpack5": "^8.5.2",
        "@storybook/test": "^8.5.2",
        "@tailwindcss/container-queries": "^0.1.1",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.0.1",
        "@testing-library/user-event": "^14.5.2",
        "@types/jest": "29.5.11",
        "@types/node": "^20.10.0",
        "@types/pako": "^2.0.3",
        "@types/react": ">=16.8.0",
        "@types/react-dom": ">=16.8.0",
        "@types/uuid": "^9.0.7",
        "@typescript-eslint/eslint-plugin": "^8.22.0",
        "@typescript-eslint/parser": "^8.22.0",
        "autoprefixer": "^10.4.16",
        "babel-loader": "^9.1.3",
        "css-loader": "^7.1.2",
        "eslint": "^9.19.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.3",
        "eslint-plugin-react": "^7.37.4",
        "eslint-plugin-react-hooks": "^5.1.0",
        "globals": "^15.14.0",
        "highlight.js": "10.7.3",
        "husky": "^8.0.3",
        "identity-obj-proxy": "^3.0.0",
        "jest": "29.7.0",
        "jest-canvas-mock": "^2.5.2",
        "jest-environment-jsdom": "29.7.0",
        "lowlight": "1.20.0",
        "postcss": "^8.4.31",
        "postcss-loader": "^8.1.1",
        "prettier": "3.0.3",
        "prettier-plugin-organize-imports": "^3.2.4",
        "prettier-plugin-tailwindcss": "^0.5.7",
        "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
        "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
        "react-syntax-highlighter": "15.4.5",
        "remark-gfm": "^4.0.0",
        "rollup-plugin-analyzer": "^4.0.0",
        "rollup-plugin-postcss": "^4.0.2",
        "size-limit": "^10.0.3",
        "storybook": "^8.5.2",
        "style-loader": "^4.0.0",
        "tailwindcss": "^3.3.5",
        "ts-jest": "29.1.1",
        "tsdx": "^0.14.1",
        "tslib": "^2.6.2",
        "typescript": "^5.3.2",
        "typescript-eslint": "^8.21.0",
        "webpack": "5.89.0",
        "zod": "^3.22.4"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.689.0",
        "@aws-sdk/s3-request-presigner": "^3.689.0",
        "@aws-sdk/xhr-http-handler": "^3.451.0",
        "@azure/identity": "^4.5.0",
        "@azure/msal-browser": "^3.5.0",
        "@azure/storage-blob": "^12.25.0",
        "@google-cloud/storage": "^7.14.0",
        "@microsoft/microsoft-graph-client": "^3.0.7",
        "@types/gapi": "^0.0.47",
        "babel-plugin-module-resolver": "^5.0.0",
        "clsx": "^2.1.1",
        "framer-motion": "^12.0.6",
        "load-script": "^2.0.0",
        "pako": "^2.1.0",
        "react-icons": "^4.12.0",
        "react-toastify": "^11.0.3",
        "react-webcam": "^7.2.0",
        "tailwind-merge": "^2.6.0",
        "truncate": "^3.0.0",
        "use-debounce": "^10.0.4",
        "uuid": "^9.0.1"
    },
    "pnpm": {
        "overrides": {
            "fast-xml-parser": "^4.3.2",
            "prismjs": "^1.29.0",
            "tough-cookie": "^4.1.3",
            "jsdom": "22.1.0",
            "node-notifier": "10.0.1",
            "node-fetch": "2.6.7",
            "rollup": "^3.29.4"
        }
    }
}
